{
  "name": "l45-portfolio",
  "version": "0.0.0",
  "scripts": {
    "start": "npm run build:dev:serve",
    "build:dev:serve": "gulp build:dev:serve",
    "build:prod:serve": "gulp build:prod:serve",
    "build:dev": "gulp build:dev",
    "build:prod": "gulp build:prod",
    "release": "gulp build:prod:release",
    "bundle-analyze": "ng build --prod --stats-json && webpack-bundle-analyzer ng-build/stats.json",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr:prod": "npm run build:prod && ng run l45-portfolio:server:production && npm run webpack:server",
    "build:ssr:dev": "npm run build:dev && ng run l45-portfolio:server && npm run webpack:server",
    "serve:ssr": "node dist/server/server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.4",
    "@angular/common": "^7.2.4",
    "@angular/compiler": "^7.2.4",
    "@angular/core": "^7.2.4",
    "@angular/forms": "^7.2.4",
    "@angular/http": "^7.2.4",
    "@angular/platform-browser": "^7.2.4",
    "@angular/platform-browser-dynamic": "^7.2.4",
    "@angular/platform-server": "^7.2.4",
    "@angular/router": "^7.2.4",
    "@nguniversal/express-engine": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.0",
    "core-js": "^2.5.4",
    "gulp": "^4.0.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.2.4",
    "@angular-devkit/build-angular": "^0.13.0",
    "@angular/cli": "^7.3.0",
    "@angular/compiler-cli": "^7.2.4",
    "@angular/language-service": "^7.2.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "browser-sync": "^2.26.3",
    "codelyzer": "~4.5.0",
    "gulp-bump": "^3.1.3",
    "gulp-changed": "^3.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-exec": "^3.0.2",
    "gulp-git": "^2.8.1",
    "gulp-nunjucks-render": "^2.2.2",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-zip": "^4.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "module-alias": "^2.1.0",
    "protractor": "~5.4.0",
    "run-sequence": "^2.2.1",
    "ts-loader": "^5.3.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack-cli": "^3.2.3"
  }
}
